<div class="content">
  <h1>Cohort Students: <%= @cohort %></h1>
  <h2></h2>
<table class="list-all">
  <thead>
    <tr>
      <th>Student ID</th>
      <th>Student Name</th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.id %></td>
        <td><%= student.first_name %> <%= student.last_name %></td>
    </tr>
    <% end %>
  </tbody>
</table>




</div>
